{
    "swaggerGenerator": {
        "aspNetCoreToSwagger": {
            "assemblyPaths": [
                "$(OutputPath)/MyApi.dll"
            ]
        }
    },
    "codeGenerators": {
        "swaggerToTypeScriptClient": {
            "className": "{controller}Client",
            "typeScriptVersion": 1.8,
            "template": "Fetch",
            "output": "../../client/generated/MyApiClient.ts"
        },
        "swaggerToCSharpClient": {
            "namespace": "MyClient",
            "className": "{controller}Client",
            "output": "../../client/generated/MyApiClient.cs"
        }
    }
}