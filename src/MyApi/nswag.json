{
    "swaggerGenerator": {
        "webApiToSwagger": {
            "assemblyPaths": [
                "$(OutputPath)/MyApi.dll"
            ]
        }
    },
    "codeGenerators": {
        "swaggerToTypeScriptClient": {
            "className": "{controller}Client",
            "typeScriptVersion": 1.8,
            "output": "../../client/generated/MyApi.ts"
        }
    }
}